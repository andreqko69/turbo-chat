{
  "$schema": "https://json.schemastore.org/tsconfig",
  "extends": "./base.json",
  "compilerOptions": {
    "module": "ESNext",
    "target": "es2022",
    "lib": ["dom", "dom.iterable", "esnext"],
    "jsx": "preserve",
    "forceConsistentCasingInFileNames": true,
    "moduleResolution": "node",
    "plugins": [
      {
        "name": "next"
      }
    ]
  }
}
